ductSlider`}\n            {...settings}\n          >\n            {slideImgs?.map((img) => (\n              <div className={productPageStyles.slide}>\n                <div\n                  className={productPageStyles.product}\n                  style={{\n                    backgroundImage: `url(${img})`,\n                  }}\n                ></div>\n              </div>\n            ))}\n          </Slider>\n        </div>\n        <div className={productPageStyles.description}>\n          <h2>Description</h2>\n          <p className={productPageStyles.p}>\n            Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean\n            commodo ligula eget dolor. Aenean massa. Cum sociis natoque\n            penatibus et magnis dis parturient montes, nascetur ridiculus mus.\n            Donec quam felis, ultricies nec, pellentesque eu, pretium quis.\n            Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean\n            commodo ligula eget dolor. Donec quam felis, ultricies nec,\n            pellentesque eu,\n          </p>\n          <button\n            onClick={handleAddCartItem}\n            className={\"btn-black ignoreClickOutside\"}\n            type=\"button\"\n          >\n            Add to cart\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default ProductPage;\n","/Users/maja/Desktop/Projects/fiberart-shop/src/components/Cart/Cart.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Product/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Checkout/Checkout.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/TextSection/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Form/Form.js",[],{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":44,"column":13,"nodeType":"134","endLine":44,"endColumn":54},{"ruleId":"132","severity":1,"message":"133","line":51,"col