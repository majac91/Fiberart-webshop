perOutter} ${checkoutStyles.cart}`}>\n          {!isOrdered ? (\n            <>\n              {/* CART HEADER */}\n              <div className={cartStyles.titleWrapper}>\n                <h3 className={cartStyles.title}>Your order</h3>\n              </div>\n\n              {/* CART MAIN */}\n              {cartItems.length === 0 ? (\n                <p className={cartStyles.emptyCart}>The cart is empty.</p>\n              ) : null}\n\n              {cartItems.map((item, index) => (\n                <div key={index} className={cartStyles.wrapperInner}>\n                  <img\n                    className={cartStyles.img}\n                    alt={item.name}\n                    src={item.image}\n                    aria-hidden=\"true\"\n                  />\n                  <div className={cartStyles.productInfo}>\n                    <p className={cartStyles.name}>{item.name}</p>\n                    <div className={cartStyles.priceWrapper}>\n                      <p className={cartStyles.price}>{item.price}</p>\n                      <button\n                        className={cartStyles.btn}\n                        onClick={() => onDelete(item)}\n                      >\n                        Remove\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              ))}\n\n              {/* CART FOOTER */}\n              {cartItems.map((item, index) =>\n                index === cartItems.length - 1 ? (\n                  <div className={cartStyles.totalWrapper}>\n                    <p className={cartStyles.subtotal}>\n                      Subtotal <span className={cartStyles.span}>${total}</span>\n                    </p>\n                    <p className={cartStyles.shipping}>\n                      Shipping <span className={cartStyles.span}>free</span>\n                    </p>\n                    <p className={cartStyles.total}>\n                      Total <span className={cartStyles.span}>${total}</span>\n                    </p>\n                  </div>\n                ) : null\n              )}\n            </>\n          ) : (\n            <SubmitMsg />\n          )}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Checkout;\n","/Users/maja/Desktop/Projects/fiberart-shop/src/components/TextSection/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/ScrollToTop.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/About/AboutPage.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Contact/Contact.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/SubmitMsg/SubmitMsg.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/hooks/useOnIntersection.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/About.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/BrandMessage.js",["171","172","173"],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Newsletter/Newsletter.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Featured.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/MissionStatement.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/ProductCategories.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/TwoPhotosSection/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/TwoPhotosSection/TwoPhotosSection.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Slider/Slider.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Shop/components/FAQ/Faq.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Checkout/components/Form/CheckoutForm.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Loader/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Loader/Loader.js",[],"/Users/maja/Desktop/Projects/fiberart-sho