n\n  function handleAddToCart() {\n    setCartCount((prev) => prev + 1);\n  }\n\n  function handleToggleCart() {\n    setCartIsOpen((prev) => !prev);\n  }\n\n  function deleteCartItem(item) {\n    const items = cartItems.filter((i) => i !== item);\n    setCartItems(items);\n    setCartCount(items.length);\n  }\n\n  function handleToggleNav() {\n    setBurgerIsOpen((prev) => !prev);\n  }\n\n  let total = cartItems.reduce(\n    (acc, curr) => acc + parseInt(curr.price.slice(1)),\n    0\n  );\n\n  return (\n    <Router>\n      <ScrollToTop />\n      <Nav\n        onBurgerClick={handleToggleNav}\n        clickOutsideRef={clickOutsideNav}\n        burgerIsOpen={burgerIsOpen}\n        cartCount={cartCount}\n        onCartClick={handleToggleCart}\n        setCartIsOpen={setCartIsOpen}\n      />\n      <Cart\n        clickOutsideRef={clickOutsideCart}\n        total={total}\n        onDelete={deleteCartItem}\n        onCartClick={handleToggleCart}\n        cartIsOpen={cartIsOpen}\n        cartItems={cartItems}\n      />\n      <Switch>\n        <Route exact path=\"/\">\n          <Main submitted={submitted} />\n        </Route>\n\n        <Route exact path=\"/shop\">\n          <Shop></Shop>\n        </Route>\n\n        <Route exact path=\"/product/:id\">\n          <ProductPage\n            setCartIsOpen={setCartIsOpen}\n            onCartClick={handleToggleCart}\n            onAddToCart={handleAddToCart}\n            cartItems={cartItems}\n            setCartItems={setCartItems}\n          ></ProductPage>\n        </Route>\n\n        <Route exact path=\"/about\">\n          <AboutPage />\n        </Route>\n\n        <Route exact path=\"/checkout\">\n          <CheckoutPage\n            submitted={submitted}\n            setSubmitted={setSubmitted}\n            total={total}\n            onDelete={deleteCartItem}\n            cartItems={cartItems}\n          />\n        </Route>\n\n        <Route exact path=\"/contact\">\n          <ContactPage submitted={submitted} setSubmitted={setSubmitted} />\n        </Route>\n      </Switch>\n      <Footer paragraph=\"I help you create a home you love, so you can relax and refresh in a pure and simple space.\"></Footer>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/maja/Desktop/Projects/fiberart-shop/src/reportWebVitals.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/firebase/config.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Nav/Nav.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Shop/Shop.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Footer/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/Main.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Footer/Footer.js",["151","152","153","154"],"import React from \"react\";\nimport textStyles from \"../TextSection/text-section.module.css\";\nimport footerStyles from \"./footer.module.css\";\n\nimport logo from \"../../img/logo.png\";\nimport img1 from \"../../img/social1.jpg\";\nimport img2 from \"../../img/social2.jpg\";\nimport img3 from \"../../img/social3.jpg\";\nimport img4 from \"../../img/social4.jpg\";\n\nimport facebookbtn from \"../../icons/facebook.png\";\nimport twitterbtn from \"../../icons/twitter.png\";\nimport pinterestbtn from \"../../icons/pinterest.png\";\nimport tumblrbtn from \"../../icons/tumblr.png\";\n\nexport default function Footer(props) {\n  return (\n    <>\n      <div className={`${footerStyles.container} d-flex container`}>\n        <div className={` ${footerStyles.imgWrapper}`}>\n          <div className={footerStyles.imgWrapperInner}>\n            <img\n              alt=\"beige macrame wall hanging\"\n              src={img1}\n              className={`${footerStyles.img1} ${footerStyles.img}`}\n            ></img>\n            <img\n              src={img2}\n              alt=\"person painting using watercolors\"\n              className={`${footerStyles.img2} ${footerStyles.img}`}\n            ></img>\n            <img\n              src={img3}\n              alt=\"dry bouquet detail\"\n              className={`${footerStyles.img3} ${footerStyles.img}`}\n            ></img>\n            <img\n              src={img4}\n              alt=\"artist's studio\"\n              className={`${footerStyles.img4} ${footerStyles.img}`}\n            ></img>\n          </div>\n        </div>\n\n        <div className={footerStyles.textWrapper}>\n          <div className={footerStyles.innerTextWrapper}>\n            <p className={`${textStyles.textClass} ${textStyles.caption}`}>\n              <img alt=\"logo\" src={logo} className={footerStyles.logo}></img>\n            </p>\n            <p className=\"p-small text-column\">\n              <span>{props.paragraph} </span>\n            </p>\n            <div>\n              <a className={footerStyles.link} href=\"\">\n                <img\n                  className={footerStyles.socialIcon}\n                  alt=\"go to facebook page\"\n                  src={facebookbtn}\n                />\n              </a>\n              <a className={footerStyles.link} href=\"\">\n                <img\n                  className={footerStyles.socialIcon}\n                  alt=\"go to twitter profile\"\n                  src={twitterbtn}\n                />\n              </a>\n              <a className={footerStyles.link} href=\"\">\n                <img\n                  className={footerStyles.socialIcon}\n                  alt=\"go to pinterest page\"\n                  src={pinterestbtn}\n                />\n              </a>\n              <a className={footerStyles.link} href=\"\">\n                <img\n                  className={footerStyles.socialIcon}\n                  alt=\"go to tumblr page\"\n                  src={tumblrbtn}\n                />\n              </a>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div className={`${footerStyles.copy} d-flex container`}>\n        <p className=