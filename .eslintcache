.forEach((entry) => {\n        if (entry.isIntersecting) {\n          console.log(\"blalabla\");\n\n          let downloadingImage = new Image();\n          downloadingImage.onload = function () {\n            setIsIntersecting(true);\n          };\n          downloadingImage.src = imgSrc;\n        }\n      });\n    };\n    const observer = new IntersectionObserver(callback, options);\n    observer.observe(ref.current);\n\n    const currentRef = ref.current;\n    return () => observer.unobserve(currentRef);\n  }, []);\n\n  return isIntersecting;\n}\n","/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/About.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/BrandMessage.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Newsletter/Newsletter.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Featured.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/MissionStatement.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/ProductCategories.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/TwoPhotosSection/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/TwoPhotosSection/TwoPhotosSection.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Slider/Slider.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Shop/components/FAQ/Faq.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Shop/components/ProductsList/Products.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Checkout/components/Form/CheckoutForm.js",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":2,"column":20,"nodeType":"166","messageId":"167","endLine":2,"endColumn":29},{"ruleId":"168","severity":1,"message":"169","line":54,"column":15,"nodeType":"170","endLine":54,"endColumn":