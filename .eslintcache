g/SubmitMsg\";\n\nconst Checkout = ({ onClearCart, total, onDelete, cartItems }) => {\n  const [isOrdered, setIsOrdered] = useState(false);\n\n  useEffect(() => {\n    if (isOrdered) {\n      onClearCart();\n    }\n  }, [isOrdered]);\n\n  return (\n    <>\n      <Header page=\"checkout\" caption=\"Complete your order\" />\n      <main className={`${checkoutStyles.container} d-flex container`}>\n        {/* F O R M */}\n        <section className={checkoutStyles.form}>\n          <CheckoutForm setIsOrdered={setIsOrdered} cartItems={cartItems} />\n        </section>\n        {/* C A R T */}\n        <section\n          className={`${cartStyles.wrapperOutter} ${checkoutStyles.cart}`}\n        >\n          {!isOrdered ? (\n            <>\n              {/* CART HEADER */}\n              <div className={cartStyles.titleWrapper}>\n                <h3 className={cartStyles.title}>Your order</h3>\n              </div>\n\n              {/* CART MAIN */}\n              {cartItems.length === 0 ? (\n                <p className={cartStyles.emptyCart}>The cart is empty.</p>\n              ) : null}\n\n              {cartItems.map((item, index) => (\n                <div key={index} className={cartStyles.wrapperInner}>\n                  <img\n                    className={cartStyles.img}\n                    alt={item.name}\n                    src={item.image}\n                    aria-hidden=\"true\"\n                  />\n                  <div className={cartStyles.productInfo}>\n                    <p className={cartStyles.name}>{item.name}</p>\n                    <div className={cartStyles.priceWrapper}>\n                      <p className={cartStyles.price}>{item.price}</p>\n                      <button\n                        className={cartStyles.btn}\n                        onClick={() => onDelete(item)}\n                      >\n                        Remove\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              ))}\n\n              {/* CART FOOTER */}\n              {cartItems.map((item, index) =>\n                index === cartItems.length - 1 ? (\n                  <div className={cartStyles.totalWrapper}>\n                    <p className={cartStyles.subtotal}>\n                      Subtotal <span className={cartStyles.span}>${total}</span>\n                    </p>\n                    <p className={cartStyles.shipping}>\n                      Shipping <span className={cartStyles.span}>free</span>\n                    </p>\n                    <p className={cartStyles.total}>\n                      Total <span className={cartStyles.span}>${total}</span>\n                    </p>\n                  </div>\n                ) : null\n              )}\n            </>\n          ) : (\n            <SubmitMsg />\n          )}\n        </section>\n      </main>\n    </>\n  );\n};\n\nexport default Checkout;\n","/Users/maja/Desktop/Projects/fiberart-shop/src/components/ScrollToTop.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/About/AboutPage.js",["157"],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Contact/Contact.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/SubmitMsg/SubmitMsg.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/hooks/useOnIntersection.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/About.js",["158","159"],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Featured.js",["160","161"],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Main/components/Slider/Slider.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Shop/components/FAQ/Faq.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/pages/Checkout/components/Form/CheckoutForm.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Loader/index.js",[],"/Users/maja/Desktop/Projects/fiberart-shop/src/components/Loader/Loader.js",[],"/Users/maja/Desktop/Projects/fibera